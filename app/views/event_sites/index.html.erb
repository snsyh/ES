<p id="notice"><%= notice %></p>

<h1>Listing Event Sites</h1>

<table>
  <thead>
    <tr>
      <th>Event site name</th>
      <th>Prefecture type</th>
      <th>Near station</th>
      <th>Access route</th>
      <th>Host comment</th>
      <th>Carpark type</th>
      <th>Capacity</th>
      <th>Degrees of latitude</th>
      <th>Degrees of longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_sites.each do |event_site| %>
      <tr>
        <td><%= event_site.event_site_name %></td>
        <td><%= event_site.prefecture_type.name %></td>
        <td><%= event_site.near_station %></td>
        <td><%= event_site.access_route %></td>
        <td><%= event_site.host_comment %></td>
        <td><%= event_site.carpark_type.name %></td>
        <td><%= event_site.capacity %></td>
        <td><%= event_site.degrees_of_latitude %></td>
        <td><%= event_site.degrees_of_longitude %></td>
        <td><%= link_to 'Show', event_site %></td>
        <td><%= link_to 'Edit', edit_event_site_path(event_site) %></td>
        <td><%= link_to 'Destroy', event_site, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event site', new_event_site_path %>
